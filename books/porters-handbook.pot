#, no-wrap
msgid ""
".include &lt;bsd.port.pre.mk&gt;\n"
"\n"
".if ${PORT_OPTIONS:MX11} &amp;&amp; ${ARCH} == \"i386\"\n"
"PLIST_SUB+=\tX11I386=\"\"\n"
".else\n"
"PLIST_SUB+=\tX11I386=\"@comment \"\n"
".endif"
msgstr ""
".include &lt;bsd.port.pre.mk&gt;\n"
"\n"
".if ${PORT_OPTIONS:MX11} &amp;&amp; ${ARCH} == \"i386\"\n"
"PLIST_SUB+=\tX11I386=\"\"\n"
